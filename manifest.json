{
    "name": "Pokemon Showdown Tooltip",
    "version": "1.0",
    "description": "Pokemon Showdown Tooltip",
    "permissions": ["storage"],
    "browser_action": {
        "default_popup": "html/popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://play.pokemonshowdown.com/*", "http://play.pokemonshowdown,com/*"],
            "js": ["js/content.js"]
        }
    ],
    "manifest_version": 2
  }